{
	"builds": [
		{
			"src": "server/app.py",
			"use": "@vercel/python",
			"config": { "runtime": "python3.13" }
		},
		{
			"src": "client/late-bloomers/package.json",
			"use": "@vercel/static-build"
		}
	],
	"routes": [
		{
			"src": "/server/(.*)",
			"dest": "/server/app.py"
		},
		{
			"src": "/(.*)",
			"dest": "/client/late-bloomers/$1"
		}
	],
	"installCommand": "npm install",
	"buildCommand": "npm run build",
	"outputDirectory": "client/late-bloomers/"
}
